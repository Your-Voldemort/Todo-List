[run]
source = .
omit = 
    */tests/*
    */venv/*
    */todo/*
    */__pycache__/*
    */migrations/*
    test_app.py
    reset_db.py
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
